
#include <stdio.h>
#include <math.h>
#include <stdbool.h>
#include <stdlib.h>

bool checkPrime(int n){
	if ( n < 2) return false;
	for ( int i = 2 ; i <= sqrt(n) ;i++){
		if ( n % i == 0) return false;
	}
	return true;
}


int gcd(int a, int b){
	while ( b != 0){
		int r = a % b;
		a = b ;
		b = r;
	}
	return a;
}


int main(){
	int n;
	printf("Nhap so luong phan tu cua mang : ");
	scanf("%d",&n);
	int A[n];
	printf("Nhap cac gia tri cho mang A :");
	for (int i = 0 ; i < n ; i++){
		scanf("%d",&A[i]);
	}
	int count = 0;
	for( int i = 0 ; i < n ; i++){
		for ( int j = 0; j < n ; j++){
			if( j > i && checkPrime(gcd(A[i],A[j]))) {
				count ++;
			}
		}
	}
	
	printf("%d", count);
	return 0;
}
