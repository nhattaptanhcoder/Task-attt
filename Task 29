#include <stdio.h>

#define max 10000

int convertBinary(int arr[], int k){
	int i = 0;
	while (k > 0){
		arr[i++] = k % 2;
		k = k / 2;
	}
	return i;
}

int gcd(int a, int b){
	while (b != 0){
		int r = a % b;
		a = b;
		b = r;
	}
	return a;
}

int mod(int arr[], int k, int a, int n){
	int b = 1;
	int length = convertBinary(arr, k);
	int A = a;
	if (arr[0] == 1){
		b = a;
	}
	for (int i = 1; i < length; i++){
		A = (A * A) % n;
		if (arr[i] == 1){
			b = (A * b) % n;
		}
	}
	return b;
}


int isCarmichael(int n){
	if (n < 3) return 0;
	
	   int check = 0;
    for (int i = 2; i * i <= n; i++){
        if (n % i == 0){
            check = 1;
            break;
        }
    }
    
    
    if (!check) return 0;
    
	for (int a = 2; a < n; a++){
		if (gcd(a, n) == 1){
			int arr[max];
			if (mod(arr, n - 1, a, n) != 1){
				return 0;
			}
		}
	}
	return 1;
}

int main(){
	int n; 
	printf("Nhap n: ");
	scanf("%d", &n);
	int count = 0;
	for (int i = 0; i <= n; i++){
		if (isCarmichael(i)) {
			count++;
			printf("%d ", i); 
		}
	}
	
	printf("\nSo luong so Carmichael trong doan [0;%d] la: %d\n", n, count);
	return 0;
}
