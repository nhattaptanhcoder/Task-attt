#include <stdio.h>

#define max 10000




int convertBinary(int arr[], int k){
	int i = 0;
	while (k > 0){
		arr[i++] = k % 2;
		k = k / 2;
	}
	return i;
}

int mod(int k, int a, int n){
	int arr[max];
	int b = 1;
	int length = convertBinary(arr, k);
	int A = a;
	if (arr[0] == 1){
		b = a;
	}
	for (int i = 1; i < length; i++){
		A = (A * A) % n;
		if (arr[i] == 1){
			b = (A * b) % n;
		}
	}
	return b;
}


int main(){
	int n;
	printf("Nhap so n le can kiem tra : ");
	scanf("%d",&n);
	
	int t;
	printf("Nhap so vong can kiem tra : ");
	scanf("%d",&t);
	
	int a ;
	for ( int i = 0 ; i < t ; i++){
		printf("Nhap co so a : ");
		scanf("%d",&a);
		if (mod(n-1,a,n) == 1 ){
			printf("%d la so nguyen to voi co so %d \n",n,a);
		}
		else{
			printf("%d la hop so voi co so %d \n",n,a);
		}
	}
	
	
	
	return 0;
}
